// Задача 1
// Convert Celsius to Fahrenheit
function convertCtoF(celsius) {
  let fahrenheit = celsius * (9 / 5) + 32;
  return fahrenheit;
}

let p = convertCtoF(30);
console.log(p);



// Задача 2
// Reverse a String
function reverseString(str) {
  let reverStr = '';
  for (let i = str.length -1; i >= 0; i--) {
    reverStr += str[i];
  }
  return reverStr;
}

let p = reverseString("hello");
console.log(p);
